set number
syntax enable
set showcmd
set cursorline
filetype plugin indent on
set wildmenu
set tabstop=4
set softtabstop=4
set shiftwidth=4
set ignorecase
set expandtab
set autoindent
set smartindent
set laststatus=2
set colorcolumn=80

autocmd Filetype go setlocal noexpandtab shiftwidth=4

colorscheme pablo

